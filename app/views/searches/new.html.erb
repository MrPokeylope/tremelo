<%= @search.current_step%><br>

<%= @search.search_params%><br>

<%= form_tag searches_path, method: :get do %>
<%= render partial: "#{@search.current_step}_step", locals: {results: @results}%>
<br>
<br>
<br>
<%= submit_tag "Previous", name: "back" unless @search.first_step? %>
<%= submit_tag "Next" unless @search.last_step? %>
<br>
<br>
<% end%>


